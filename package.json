{
  "name": "aztec-nextjs-starter",
  "version": "1.0.0",
  "description": "Aztec NextJS boilerplate code",
  "author": {
    "name": "Satyam Bansal <satyambnsal@gmail.com>",
    "url": "git@github.com:satyambnsal/aztec-nextjs-starter.git"
  },
  "scripts": {
    "circuits::build": "cd circuits && aztec-nargo build",
    "circuits::compile": "cd circuits && aztec-nargo compile",
    "circuits::test": "cd circuits && aztec-nargo compile && aztec test",
    "circuits::gate-count": "sh scripts/build-gates-report.sh",
    "circuits::codegen": "cd circuits && aztec codegen target --outdir ../src/artifacts",
    "deploy-contract": "bun run scripts/deploy-contract.ts",
    "register-contract": "bun run scripts/register-contract.ts",
    "cli": "bun run scripts/cli.ts",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "keywords": [
    "NextJS",
    "Aztec",
    "Noir"
  ],
  "license": "MIT",
